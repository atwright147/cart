{
  "name": "shopping-cart-ts",
  "version": "1.0.0",
  "description": "A simple shopping cart for Node or Browser, written in TypeScript",
  "main": "index.js",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "coverage:lcov": "nyc report --reporter=text-lcov > ./coverage/lcov.info",
    "coverage:json": "nyc report --reporter=json",
    "coverage": "npm run coverage:lcov && npm run coverage:json",
    "test": "nyc mocha './src/**/*.spec.ts' --opts ./mocha.opts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.5",
    "@types/sinon": "^7.5.0",
    "@types/uuid": "^3.4.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0",
    "sinon-register": "^1.0.1",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "node-uuid": "^1.4.8"
  }
}
