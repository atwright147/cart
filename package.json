{
  "name": "@atwright147/cart",
  "version": "1.0.0",
  "description": "A simple shopping cart for Node or Browser, written in TypeScript",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/cart.module.js",
  "unpkg": "dist/cart.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "microbundle --external none",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --project tsconfig.typecheck.json --noEmit",
    "coverage:lcov": "nyc report --reporter=lcovonly",
    "coverage:json": "nyc report --reporter=json",
    "coverage": "npm run coverage:lcov && npm run coverage:json",
    "test": "cross-env NODE_ENV=test TS_NODE_PROJECT='tsconfig.test.json' nyc mocha --exit"
  },
  "keywords": [
    "cart",
    "shopping",
    "shopping cart"
  ],
  "author": "Andy Wright <atwright147+cart@gmail.com",
  "license": "MIT",
  "dependencies": {
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.17",
    "@types/chai-uuid": "^1.0.2",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.1",
    "@types/node-uuid": "0.0.28",
    "@types/sinon": "^10.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "chai": "^4.3.4",
    "chai-exclude": "^2.0.3",
    "chai-uuid": "^1.0.6",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "microbundle": "^0.13.0",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "sinon": "^10.0.0",
    "sinon-register": "^1.0.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atwright147/cart.git"
  },
  "bugs": {
    "url": "https://github.com/atwright147/cart/issues"
  },
  "homepage": "https://github.com/atwright147/cart#readme"
}
