{
  "name": "shopping-cart-ts",
  "version": "1.0.0",
  "description": "A simple shopping cart for Node or Browser, written in TypeScript",
  "main": "index.js",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "coverage:lcov": "nyc report --reporter=lcovonly",
    "coverage:json": "nyc report --reporter=json",
    "coverage": "npm run coverage:lcov && npm run coverage:json",
    "test": "cross-env NODE_ENV=test nyc mocha --exit"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "@types/chai": "^4.2.12",
    "@types/node": "^14.6.0",
    "@types/node-uuid": "0.0.28",
    "@types/sinon": "^9.0.4",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "sinon": "^9.0.3",
    "sinon-register": "^1.0.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "node-uuid": "^1.4.8"
  }
}
